{
  "parser": "@typescript-eslint/parser",
  "plugins": ["solid", "prettier"],
  "extends": [
    "eslint:recommended",
    "plugin:solid/typescript",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error"
  },
  "env": {
    "browser": true,
    "es2021": true
  }
}
